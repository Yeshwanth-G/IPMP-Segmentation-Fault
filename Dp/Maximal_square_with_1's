//DP:O(n*m),O(n*m)
int maxSquare(int n, int m, vector<vector<int>> mat){
       if(n==1&&m==1)return mat[0][0]==1;
       int mx=0;
       for(int i=n-1;i>=0;i--){
           for(int j=m-1;j>=0;j--){
               if(mat[i][j]==0)continue;
              if(i+1<n&&j+1<m) mat[i][j]=min(mat[i+1][j],min(mat[i+1][j+1],mat[i][j+1]))+1;
               if(mat[i][j]>mx)mx=mat[i][j];
           }
       }
       return mx;
    }
